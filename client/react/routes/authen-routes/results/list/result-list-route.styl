.result-list-route{
  position relative
  .result-list-table{
    table{
      table-layout: fixed;
    }
  }
  .m-portlet{
    padding-bottom 50px
  }


  .create-result{
    font-size 15px
    font-weight 500
    padding 10px 20px
    background-color primary
    border-color primary
    &:hover{
      background-color darken(primary, 20%)
    }
    i{
      color white
      font-size 20px
      margin-right 10px
      font-weight 550 !important
    }
  }
}