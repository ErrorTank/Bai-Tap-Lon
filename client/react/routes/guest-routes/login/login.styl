.login-route{
  width 100vw
  height 100vh
  background-image: url("./assets/img/grey.png");
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  .login-panel-wrap{
    width 100%
    height 100%
    display flex
    align-items center
    justify-content center
    .login-panel{
      padding 10px
      border-radius 5px
      background-color white
      width 500px
    }
  }
}
